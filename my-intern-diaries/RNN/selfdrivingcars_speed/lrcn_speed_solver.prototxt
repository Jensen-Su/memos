net: "./examples/selfdrivingcars_speed/lrcn_speed.prototxt"

test_iter: 2 
test_interval: 100
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 10000
display: 20
max_iter: 30000
momentum: 0.9
weight_decay: 0.005
snapshot: 5000
snapshot_prefix: "snapshots_lrcn_speed"
solver_mode: GPU
device_id: 0
average_loss: 1000
clip_gradients: 10


